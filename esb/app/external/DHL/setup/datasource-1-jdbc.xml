<?xml version="1.0" encoding="UTF-8"?>
<jdbc-data-source xmlns="http://xmlns.oracle.com/weblogic/jdbc-data-source">
  <name>kallsonys</name>
  <jdbc-driver-params>
    <url>jdbc:mysql://127.0.0.1:3306/kallsonys_backend</url>
    <driver-name>com.mysql.jdbc.Driver</driver-name>
    <properties>
      <property>
        <name>user</name>
        <value>kallsonys</value>
      </property>
      <property>
        <name>password</name>
        <value/>
      </property>
    </properties>
  </jdbc-driver-params>
  <jdbc-data-source-params>
    <jndi-name>kallsonys</jndi-name>
  </jdbc-data-source-params>
</jdbc-data-source>
