<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.kallsonys.com.co/cim/businessentities/creditcard" targetNamespace="http://www.kallsonys.com.co/cim/businessentities/creditcard" elementFormDefault="unqualified">
	<complexType name="CreditCard">
		<sequence>
			<element name="cardType" type="tns:cardTypeEnum" minOccurs="0" maxOccurs="1"/>
			<element name="cardNumber" type="string" minOccurs="0" maxOccurs="1"/>
		</sequence>
	</complexType>
	<simpleType name="cardTypeEnum">
		<restriction base="string">
			<enumeration value="DISCOVER"/>
			<enumeration value="VISA"/>
			<enumeration value="MASTERCARD"/>
			<enumeration value="AMERICANEXPRESS"/>
		</restriction>
	</simpleType>
</schema>
