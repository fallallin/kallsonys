<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:excgen="http://www.kallsonys.com.co/cim/common/exceptiongeneric" xmlns:tns="http://www.kallsonys.com.co/cim/message/payment/v1" name="FactoriesQuotesOrders" targetNamespace="http://www.kallsonys.com.co/cim/message/payment/v1">
	<types>
		<schema targetNamespace="http://www.kallsonys.com.co/cim/message/payment/v1" xmlns="http://www.w3.org/2001/XMLSchema">
			<include schemaLocation="../../xsd/Transport.xsd"/>
			<import namespace="http://www.kallsonys.com.co/cim/common/exceptiongeneric" schemaLocation="../../../../Common/exceptiongeneric.xsd"/>
		</schema>
	</types>
	<wsdl:message name="Payment_in">
		<wsdl:part name="PaymentRq" element="tns:PaymentRq"/>
	</wsdl:message>
	<wsdl:message name="Payment_out">
		<wsdl:part name="PaymentRs" element="tns:PaymentRs"/>
	</wsdl:message>
	<wsdl:message name="Payment_fault1">
		<wsdl:part name="exceptionGeneric" element="excgen:exceptionGeneric"/>
	</wsdl:message>
	<wsdl:portType name="PaymentPortType">
		<wsdl:operation name="payment">
			<wsdl:input name="Payment_Input" message="tns:Payment_in"/>
			<wsdl:output name="Payment_Output" message="tns:Payment_out"/>
			<wsdl:fault name="Payment_Fault" message="tns:Payment_fault1"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="PaymentBinding" type="tns:PaymentPortType">
		<soap12:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="payment">
			<soap12:operation soapAction="payment"/>
			<wsdl:input name="Payment_Input">
				<soap12:body parts="PaymentRq" use="literal"/>
			</wsdl:input>
			<wsdl:output name="Payment_Output">
				<soap12:body parts="PaymentRs" use="literal"/>
			</wsdl:output>
			<wsdl:fault name="Payment_Fault">
				<soap12:fault name="Payment_Fault" use="literal"/>
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="PaymentService">
		<wsdl:port name="PaymentPort" binding="tns:PaymentBinding">
			<soap12:address location="http://localhost:8080/message/"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
