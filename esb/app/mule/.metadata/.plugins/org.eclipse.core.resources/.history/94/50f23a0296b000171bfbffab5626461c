package co.com.kallsonys.esb.util;

import java.io.IOException;

import javax.security.auth.callback.Callback;
import javax.security.auth.callback.CallbackHandler;
import javax.security.auth.callback.UnsupportedCallbackException;

import org.apache.ws.security.WSPasswordCallback;

public class UtilHeaderFilterInterceptor  implements CallbackHandler {
	public UtilHeaderFilterInterceptor() {
		super();
	}

	@Override
	public void handle(Callback[] callbacks) throws IOException, UnsupportedCallbackException {
		System.out.println("TASHuer y su WS-Security!!!!!!");
		WSPasswordCallback pc = (WSPasswordCallback) callbacks[0];
		System.out.println(pc.getIdentifier());
	}

}
