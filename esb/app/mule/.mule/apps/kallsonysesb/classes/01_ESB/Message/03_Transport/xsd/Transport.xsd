<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:crecar="http://www.kallsonys.com.co/cim/businessentities/creditcard" xmlns:this="http://www.kallsonys.com.co/cim/message/payment/v1" targetNamespace="http://www.kallsonys.com.co/cim/message/payment/v1">
	<import namespace="http://www.kallsonys.com.co/cim/businessentities/creditcard" schemaLocation="../../../BusinessEntities/CreditCard.xsd"/>
	<element name="PaymentRq">
		<complexType>
			<complexContent>
				<restriction base="crecar:CreditCard">
					<sequence>
						<element name="cardType" type="crecar:cardTypeEnum" minOccurs="1" maxOccurs="1"/>
						<element name="cardNumber" type="string" minOccurs="1" maxOccurs="1"/>
					</sequence>
				</restriction>
			</complexContent>
		</complexType>
	</element>
	<element name="PaymentRs">
		<complexType>
			<complexContent>
				<restriction base="crecar:CreditCard">
					<sequence>
						<element name="valid" type="boolean" minOccurs="1" maxOccurs="1"/>
					</sequence>
				</restriction>
			</complexContent>
		</complexType>
	</element>
</schema>
